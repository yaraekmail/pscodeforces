#include<iostream>
using namespace std;
 
int main() {
    const int N =1e6 +5;
    int a;
    cin >> a;
    
    int nums[N];  
   
    for(int i = 0; i < a; i++) {
        cin >> nums[i];
    }
    
    
    for(int i = a - 1; i >= 0; i--) {
        cout << nums[i] << " ";  
    }
     
    return 0;
}