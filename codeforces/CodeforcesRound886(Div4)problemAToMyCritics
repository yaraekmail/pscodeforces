#include <iostream>
using namespace std;

int main()
{
    int a, b, c, x;
    cin >> x;
    for (int i = 0; i < x; i++)
    {
        cin >> a >> b >> c;
        if (a + b >= 10 || a + c >= 10 || b + c >= 10)
            cout << "YES"<<endl;
        else
            cout << "NO"<<endl;
    }
}