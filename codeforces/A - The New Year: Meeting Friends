https://codeforces.com/contest/723/problem/A
#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int a, b, c;
    cin >> a >> b >> c;
    int q = max({a, b, c});
    int w = min({a, b, c});
    cout << q - w;
    return 0;
}
