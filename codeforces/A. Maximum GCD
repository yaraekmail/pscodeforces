https://codeforces.com/contest/1370/problem/A

#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int a;
        cin >> a;
        if (a == 1)
            cout << a << endl;
        else
            cout << a / 2 << endl;
    }
 
    return 0;
}
