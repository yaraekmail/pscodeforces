https://codeforces.com/contest/1368/problem/A
#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int a, b, c, c1 = 0;
        cin >> a >> b >> c;
        while (max(a, b) <= c)
        {

            if (a < b)
                a += b;
            else
                b += a;
            c1++;
        }
        cout << c1 << endl;
    }

    return 0;
}
