#include<iostream>
using namespace std;
 
int main() {
    int a;
    cin >> a;
    
    int nums[1000];  
   
    for(int i = 0; i < a; i++) {
        cin >> nums[i];
    }
    
    
    for(int i = a - 1; i >= 0; i--) {
        cout << nums[i] << " ";  
    }
    cout << endl;  
    return 0;
}
