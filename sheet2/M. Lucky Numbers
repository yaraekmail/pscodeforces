https://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/M

#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int a, b;
    cin >> a >> b;
    bool r = true;
    int reversed = 0;
    for (int q = a; q <= b; q++)
    {
        int c1 = 0, c2 = 0, i = q, d = q;
 
        while (i > 0)
        {
            int digit = i % 10;
            c1++;
            i = i / 10;
        }
        while (d > 0)
        {
            int digit = d % 10;
            if (digit == 4 || digit == 7)
                c2++;
 
            d = d / 10;
        }
        if (c1 == c2)
        {
            cout << q << " ";
            r = false;
        }
    }
    if (r)
        cout << -1;
 
    return 0;
}
