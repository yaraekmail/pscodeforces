https://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/S

#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int x, y, c = 0;
        cin >> x >> y;
        for (int i = min(x, y) + 1; i < max(x, y); i++)
            if (i % 2 != 0)
                c += i;

        cout << c << endl;
    }

    return 0;
}
