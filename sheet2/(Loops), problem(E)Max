 
#include<iostream>
using namespace std;
 
int main() {
    int a,x=0;
    cin >> a; 
    
    int nums[1000]; 
    for(int i = 0; i < a; i++) {
        cin >> nums[i];      }
    
     for(int i = a - 1; i >= 0; i--) {
     if(nums[i]>x)
     x=nums[i];
     }
     cout<<x;
    return 0;
}