#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    char s;
    int n;
    cin >> s >> n;
    while (n--)
    {
        int a;
        cin >> a;
        for (int i = 0; i < a; i++)

            cout << s;
        cout << endl;
    }

    return 0;
}
